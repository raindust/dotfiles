# enviroment settings
export LC_ALL=en_US.UTF-8
export LANG=en_US.UTF-8

# enviroment variables
export GOPATH=$HOME/dev
export GOBIN=$GOPATH/bin
export GOPROXY=https://mirrors.aliyun.com/goproxy
